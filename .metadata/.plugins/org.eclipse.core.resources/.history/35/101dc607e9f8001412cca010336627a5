package hr.fer.zemris.java.gui.calc.buttons.functions;

public class Logarithm10Function extends FunctionButton {

	public Logarithm10Function() {
		super("log", "10^x");
	}

	@Override
	protected double normalApply(double number) {
		return Math.log10(number);
	}

	@Override
	protected double invertedApply(double number) {
		return Math.pow(number, 10);
	}
	
}
