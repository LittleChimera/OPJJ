package hr.fer.zemris.java.gui.layouts;

public class RCPosition {
	
	private int row;
	private int column;

	public RCPosition(int row, int column) {
		super();
		this.row = row;
		this.column = column;
	}
	
	public int getColumn() {
		return column;
	}
	
	public int getRow() {
		return row;
	}
	
	@Override
	public boolean equals(Object obj) {
		if (!(obj instanceof RCPosition)) {
			return false;
		}
		
	}
	
}
