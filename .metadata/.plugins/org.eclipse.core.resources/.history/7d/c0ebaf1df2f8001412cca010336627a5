package hr.fer.zemris.java.gui.calc.buttons.functions;


public class SinusButton extends FunctionButton {

	public SinusButton() {
		super("sin", "asin");
	}

	@Override
	public double invertedApply(double number) {
		return Math.asin(number);
	}

	@Override
	protected double normalApply(double number) {
		return Math.sin(number);
	}

}
