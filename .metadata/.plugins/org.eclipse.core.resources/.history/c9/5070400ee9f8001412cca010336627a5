package hr.fer.zemris.java.gui.calc.buttons.functions;

public class ReciprocalFunction extends FunctionButton {
	
	public ReciprocalFunction() {
		super("1/x", "1/x");
	}

	@Override
	protected double normalApply(double number) {
		return 1/number;
	}

	@Override
	protected double invertedApply(double number) {
		return normalApply(number);
	}
	
	
	
}
